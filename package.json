{
  "name": "@moviesup/uicore",
  "version": "1.4.7",
  "description": "uicore for moviesup project ",
  "main": "dist/index.bundle.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts, tsx}\" && stylelint \"src/**/*.css\" && prettier --check \"src/**/*.{ts, tsx, css}\"",
    "ui:icons": "node ./scripts/build-icons.js > ./src/icons/CarlosIcons.ts",
    "ui:build": "NODE_ENV=production webpack",
    "test": "jest --no-cache ./src",
    "test:ci": "jest --no-cache ./src --ci --coverage --reporters=jest-junit --reporters=default --runInBand --silent",
    "prebuild": "npm run clean",
    "build": "npm run ui:build && npm run types:copy",
    "clean": "rm -rf docs/static dist",
    "types:copy": "echo 'Adhoc type copy...'; mv dist/src dist/; rm -rf dist/src",
    "fmt": "prettier --write \"./src/**/*.{ts,tsx,css,scss}\"",
    "prepare": "npm run lint && npm run prebuild && npm run build",
    "release": "standard-version"
  },
  "repository": "https://github.com/carlosdumar/uicoreMoviesUp",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "movies",
    "uicore"
  ],
  "author": "Carlos Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlosdumar/uicoreMoviesUp/issues"
  },
  "homepage": "https://github.com/carlosdumar/uicoreMoviesUp#readme",
  "dependencies": {
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.0",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.0.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "sugarss": "^4.0.1",
    "utility-types": "^3.10.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-node-externals": "^3.0.0",
    "yaml-loader": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/lint-commit-message.js \"$(cat $HUSKY_GIT_PARAMS)\""
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint",
      "prettier --check",
      "sh script/typecheck-staged.sh"
    ],
    "*.css": [
      "prettier --check",
      "stylelint"
    ]
  },
  "files": [
    "dist/*"
  ]
}
